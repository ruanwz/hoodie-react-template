      "node_modules/react",
      "node_modules/react-bootstrap",
      "node_modules/lodash-node",
      "node_modules/fluxxor"
    ],
    "rootDir": "www/assets/scripts",
    "testFileExtensions": [
      "js",
      "coffee"
