  },
  "hoodie": {
    "plugins": [
      "hoodie-plugin-appconfig",
      "hoodie-plugin-email",
      "hoodie-plugin-users"
    ]
  },
  "devDependencies": {
    "coffeeify": ">=0.0.0",
    "gulp": "^3.8.5",
    "gulp-browserify": "^0.5.0",
    "gulp-jshint": "^1.6.3",
    "gulp-rename": "^1.2.0",
